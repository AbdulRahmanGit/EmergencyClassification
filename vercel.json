{
    "builds": [
        
        {
            "src": "EmergencyClassification/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "500mb",
                "runtime": "python3.9"
              
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "EmergencyClassification/wsgi.py"
        }
    ]
}
