{
    "builds": [{
        "src": "EmergencyClassification/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.7",
        "includeFiles": ["path0/requirements.txt"],
        "installCommand": "apt-get update && apt-get install -y python3-distutils && pip install --upgrade setuptools && pip install -r requirements.txt"
      
    }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "EmergencyClassification/wsgi.py"
        }
    ]
}
